{
    executable: {{{ROOT_DIR}}}/bin/baker-next
    args: [
        run
        vdf
        {{#configuration.VDF_STARTUP_ARGS}}
        {{.}}
        {{/configuration.VDF_STARTUP_ARGS}}
    ]
    environment: {
        HOME: {{{ROOT_DIR}}}/data
        TEZOS_LOG: "* -> {{{model.VDF_LOG_LEVEL}}}"
    }
    restart: always
    restart_delay: 20
}