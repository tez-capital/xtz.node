{
    executable: {{{ROOT_DIR}}}/bin/node
    args: [
        run
        --rpc-addr
        {{{model.RPC_ADDR}}}
        {{#configuration.STARTUP_ARGS}}
        {{.}}
        {{/configuration.STARTUP_ARGS}}
    ]
    environment: {
        HOME: {{{ROOT_DIR}}}/data
        TEZOS_LOG: "* -> {{{TEZOS_LOG_LEVEL}}}"
    }
    restart: always
    restart_delay: 20
}